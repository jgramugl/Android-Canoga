        </activity>
        <activity android:name=".view.BoardSize" >
        </activity>
        <activity android:name=".view.LoadGame" >
        </activity>
        <activity android:name=".view.StartingRoll" >
        </activity>
        <activity android:name=".view.SaveGame" >
