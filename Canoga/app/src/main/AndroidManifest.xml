        <activity android:name=".view.HumanGame" >
        </activity>
        <activity android:name=".view.ComputerGame" >
        </activity>
        <activity android:name=".view.ScoreScreen" >
        </activity>
        <activity android:name=".view.WinnersScreen" >
        </activity>
    </application>

</manifest>
